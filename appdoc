--celery

 python -m celery -A utils.celery_worker status

  python -m celery -A utils.celery_worker worker --loglevel=info

 python -m celery -A tasks worker --loglevel=info --detach

 python -m celery -A utils.celery_worker.celery worker --loglevel=info --detach

 python -m celery -A tasks worker --loglevel=info --detach --logfile=celery_worker.log

 --concurrency=2
 
 ps aux | grep 'celery'
 pkill -f 'celery worker'
 pkill -f 'celery -A tasks worker'
